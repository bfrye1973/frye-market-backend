{
  "version": "r12-sectors-10m",
  "bindings": {
    "feed": "https://frye-market-backend-1.onrender.com/live/intraday",
    "fields": {
      "sector": "sector",
      "breadth": "breadth_pct",
      "momentum": "momentum_pct",
      "nh": "nh",
      "nl": "nl",
      "up": "up",
      "down": "down"
    }
  },
  "order": [
    "Information Technology",
    "Materials",
    "Health Care",
    "Communication Services",
    "Real Estate",
    "Energy",
    "Consumer Staples",
    "Consumer Discretionary",
    "Financials",
    "Utilities",
    "Industrials"
  ],
  "rules": {
    "tiltExpr": "(breadth + momentum) / 2",
    "outlook": {
      "bullish": "breadth>=55 && momentum>=55",
      "bearish": "breadth<=45 && momentum<=45"
    },
    "gradeThresholds": {
      "ok": ">=60",
      "warn": ">=50",
      "danger": "<50"
    }
  },
  "ui": {
    "palette": { "ok": "ok", "warn": "warn", "danger": "danger" },
    "showNetNH": true
  }
}
